<p id="notice"><%= notice %></p>

<h1>Listing Survey Instances</h1>

<table>
  <thead>
    <tr>
      <th>Patient</th>
      <th>Survey</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @survey_instances.each do |survey_instance| %>
      <tr>
        <td><%= survey_instance.patient_id %></td>
        <td><%= survey_instance.survey_id %></td>
        <td><%= link_to 'Show', survey_instance %></td>
        <td><%= link_to 'Edit', edit_survey_instance_path(survey_instance) %></td>
        <td><%= link_to 'Destroy', survey_instance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey instance', new_survey_instance_path %>
